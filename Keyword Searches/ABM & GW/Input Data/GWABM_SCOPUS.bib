@ARTICLE{Farhadi2016348,
author={Farhadi, S. and Nikoo, M.R. and Rakhshandehroo, G.R. and Akhbari, M. and Alizadeh, M.R.},
title={An agent-based-nash modeling framework for sustainable groundwater management: A case study},
