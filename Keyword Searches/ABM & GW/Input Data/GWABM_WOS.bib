
@article{ ISI:000077753300004,
Author = {Teles, V and de Marsily, G and Perrier, E},
Title = {{A new approach for modelling sediment deposition in an alluvial plain in
   order to display its heterogeneity}},
Journal = {{COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES}},
