Title = {{Who Shares Fake News in Online Social Networks? An Agent-based Model of
   Different Personality Models and Behaviors in Social Networks}},
Booktitle = {{ACM UMAP `19: PROCEEDINGS OF THE 27TH ACM CONFERENCE ON USER MODELING,
   ADAPTATION AND PERSONALIZATION}},
Year = {{2019}},
Pages = {{234-242}},
