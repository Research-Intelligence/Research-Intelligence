@article{ ISI:000431505200005,
Author = {van Vuuren, B. J. and Potgieter, L. and van Vuuren, J. H.},
Title = {{An agent-based simulation model of Eldana saccharina Walker}},
Journal = {{NATURAL RESOURCE MODELING}},
Year = {{2018}},
