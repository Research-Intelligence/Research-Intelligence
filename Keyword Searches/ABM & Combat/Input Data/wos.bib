@inproceedings{ ISI:000310776201077,
Author = {Kane, Michael B. and Lynch, Jerome P.},
Book-Group-Author = {{IEEE}},
Title = {{An Agent-Based Model-Predictive Controller for Chilled Water Plants
   using Wireless Sensor and Actuator Networks}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
