@inproceedings{ ISI:000263939600002,
Author = {Leamy, Michael J.},
Book-Group-Author = {{ASME}},
Title = {{A SYSTEMATIC APPROACH FOR ALIGNING ODE AND AGENT-BASED MODELS:
   APPLICATION TO EPIDEMIOLOGICAL MODELING}},
Booktitle = {{DETC 2008: PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING
   TECHNICAL CONFERENCES AND COMPUTERS AND INFORMATIONAL IN ENGINEERING
   CONFERENCE, VOL 3, PTS A AND B: 28TH COMPUTERS AND INFORMATION IN
   ENGINEERING CONFERENCE}},
Year = {{2009}},
Pages = {{13-19}},
Note = {{ASME International Design Engineering Technical Conferences/Computers
   and Information in Engineering Conference, New York, NY, AUG 03-06, 2008}},
Organization = {{ASME, Design Engn Div; ASME, Comp \& Informat Engn Div}},
Abstract = {{Recent researchers active in the field of agent-based modeling have
   called for the alignment, or `docking,'of models which simulate the same
   system using different techniques. Addressing this need, the present
   article details a systematic approach for docking models described by
   (nonlinear) ordinary differential equations with analogous models
   employing autonomous agents - i.e., agent-based models (ABMs). In
   particular, the approach is demonstrated by example for an
   epidemiological SEIR (Susceptible, Exposed, Infectious, Recovered) ODE
   model with a newly-developed agent-based model. The ABM is designed such
   that the assumptions present in the ODE model are matched by the actions
   of the ABM agents and the model. In addition, less-than-transparent
   coefficients present in the ODE model are examined via difference
   equations and then mapped to appropriate agent behavior. The result is
   very good agreement in comparisons made between ODE and ABM
   model-generated tirne-histories - i.e., successful alignment. It is
   anticipated that the systematic alignment approach described herein
   should be useful for aligning ODE and ABM models in other fields of
   study - e.g., Lanchester ODE combat models vice ABM combat models.}},
