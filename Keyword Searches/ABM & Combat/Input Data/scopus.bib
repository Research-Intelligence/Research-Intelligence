@CONFERENCE{Kane20121192,
author={Kane, M.B. and Lynch, J.P.},
title={An agent-based model-predictive controller for chilled water plants using wireless sensor and actuator networks},
journal={Proceedings of the American Control Conference},
year={2012},
pages={1192-1198},
art_number={6315319},
note={cited By 7; Conference of 2012 American Control Conference, ACC 2012 ; Conference Date: 27 June 2012 Through 29 June 2012;  Conference Code:93795},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84869482273&partnerID=40&md5=eef8e78a427b285f8892efaa28dfea33},
affiliation={University of Michigan, Department of Civil and Environmental Engineering, Ann Arbor, MI 48109-2125, United States},
abstract={Reduced manning requirements and other cost reducing measures have spurred interest in automation of engineering plants onboard naval combat vessels. Furthermore, automation may increase resiliency of shipboard engineering plants when compared to the current generation of manually configured plants. This paper presents a control algorithm and deployment strategy which supervises a ship's chilled water plant to control the temperature of thermal loads (e.g., air chillers, electrical components). Redundant computation and communication capabilities motivates the use of an agent-based controller (ABC) enabled through a peer-to-peer wireless network. In the architecture presented, each agent sits on one or more digraphs corresponding to the utility generated by the fluid exiting the chilled water plant at each discharge point. Each digraph is a component of the decentralized model-predictive controller (MPC). Performance of the proposed control architecture is tested in simulation, and is shown to approach the performance of an effective, but computationally. © 2012 AACC American Automatic Control Council).},
