@article{ ISI:000435461900030,
Author = {Mih, Nathan and Brunk, Elizabeth and Chen, Ke and Catoiu, Edward and
   Sastry, Anand and Kavvas, Erol and Monk, Jonathan M. and Zhang, Zhen and
   Palsson, Bernhard O.},
Title = {{ssbio: a Python framework for structural systems biology}},
Journal = {{BIOINFORMATICS}},
Year = {{2018}},
Volume = {{34}},
Number = {{12}},
Pages = {{2155-2157}},
Month = {{JUN 15}},
Abstract = {{A Summary: Working with protein structures at the genome-scale has been
   challenging in a variety of ways. Here, we present ssbio, a Python
   package that provides a framework to easily work with structural
   information in the context of genome-scale network reconstructions,
   which can contain thousands of individual proteins. The ssbio package
   provides an automated pipeline to construct high quality genome-scale
   models with protein structures (GEM-PROs), wrappers to popular
   third-party programs to compute associated protein properties, and
   methods to visualize and annotate structures directly in Jupyter
   notebooks, thus lowering the barrier of linking 3D structural data with
   established systems workflows.}},
