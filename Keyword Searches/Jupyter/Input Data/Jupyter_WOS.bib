@article{ ISI:000497646000020,
Author = {Sandha, Sandeep Singh and Cabrera, Wellington and Al-Kateb, Mohammed and
   Nair, Sanjay and Srivastava, Mani},
Title = {{In-database Distributed Machine Learning: Demonstration using Teradata
   SQL Engine}},
Journal = {{PROCEEDINGS OF THE VLDB ENDOWMENT}},
Year = {{2019}},
Volume = {{12}},
Number = {{12}},
Pages = {{1854-1857}},
Month = {{AUG}},
Abstract = {{Machine learning has enabled many interesting applications and is
   extensively being used in big data systems. The popular approach -
   training machine learning models in frameworks like Tensorflow, Pytorch
   and Keras - requires movement of data from database engines to
   analytical engines, which adds an excessive overhead on data scientists
   and becomes a performance bottleneck for model training. In this
   demonstration, we give a practical exhibition of a solution for the
   enablement of distributed machine learning natively inside database
   engines. During the demo, the audience will interactively use Python
   APIs in Jupyter Notebooks to train multiple linear regression models on
   synthetic regression datasets and neural network models on vision and
   sensory datasets directly inside Teradata SQL Engine.}},
