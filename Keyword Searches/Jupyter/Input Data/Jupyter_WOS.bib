@inproceedings{ ISI:000501488900196,
Author = {Tu, Xinhui and Huang, Jimmy and Luo, Jing and Zhu, Runjie and He,
   Tingting},
Book-Group-Author = {{ACM}},
Title = {{Parrot: A Python-based Interactive Platform for Information Retrieval
   Research}},
Booktitle = {{PROCEEDINGS OF THE 42ND INTERNATIONAL ACM SIGIR CONFERENCE ON RESEARCH
