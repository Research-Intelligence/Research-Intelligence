@inproceedings{ ISI:000469337500020,
Author = {Pollinger, Theresa and Kohlhase, Michael and Koestler, Harald},
Editor = {{Rabe, F and Farmer, WM and Passmore, GO and Youssef, A}},
Title = {{Knowledge Amalgamation for Computational Science and Engineering}},
