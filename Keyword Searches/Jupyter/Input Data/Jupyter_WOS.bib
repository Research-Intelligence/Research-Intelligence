@article{ ISI:000407538800050,
Author = {Bakker, Mark and Miller, Anthony D. and Morgan, Leanne K. and Werner,
   Adrian D.},
Title = {{Evaluation of analytic solutions for steady interface flow where the
   aquifer extends below the sea}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2017}},
Volume = {{551}},
Number = {{SI}},
Pages = {{660-664}},
Month = {{AUG}},
Abstract = {{A computational approach is presented for steady Dupuit interface flow
   where the aquifer extends below the sea. A detailed approach is outlined
   to determine the head at the coastline so that the solution below the
   leaky seabed may be combined with any type of steady Dupuit interface
   flow in the aquifer below the land. The method allows for any inland
   boundary condition including specified head and specified flux; cases of
   freshwater lenses caused by infiltration are also considered. The
   approach is implemented in a Python script and a Jupyter Notebook. (C)
   2017 The Authors. Published by Elsevier B.V.}},
