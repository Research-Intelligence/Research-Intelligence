@article{ ISI:000473553000010,
Author = {Nath, Tanmay and Mathis, Alexander and Chen, An Chi and Patel, Amir and
   Bethge, Matthias and Mathis, Mackenzie Weygandt},
Title = {{Using DeepLabCut for 3D markerless pose estimation across species and
   behaviors}},
