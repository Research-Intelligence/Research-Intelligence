@article{ ISI:000448957900035,
Author = {Kalinowski, L. and Goraus, J. and Slebarski, A.},
Title = {{``DistorX{''} program for analysis of structural distortions affecting
   X-ray diffraction patterns}},
Journal = {{AIP ADVANCES}},
Year = {{2018}},
Volume = {{8}},
