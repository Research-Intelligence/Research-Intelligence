@inproceedings{ ISI:000401795900086,
Author = {Evans, Benjamin D. and Nikolic, Konstantin},
Book-Group-Author = {{IEEE}},
Title = {{From Bytes to Insights with Modelling as a Service A new paradigm for
   computational modelling illustrated with PyRhO}},
