publisher={J and N Group, Ltd.},
issn={15548015},
isbn={9781510883949},
language={English},
abbrev_source_title={Proc. Symp. Appl. Geophyics Eng. Environ. Probl.},
document_type={Conference Paper},
source={Scopus},
}

