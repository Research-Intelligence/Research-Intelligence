@ARTICLE{Kumar20182383,
author={Kumar, P. and Ansarullah, S.I.},
title={Performance and evaluation of heart disease risk assessment model using machine learning classification techniques},
journal={Journal of Advanced Research in Dynamical and Control Systems},
year={2018},
volume={10},
number={2},
pages={2383-2391},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85064841120&partnerID=40&md5=2551323c9535e2f552501c9f46a3d117},
affiliation={Department of CS&IT, MANUU, Hyderabad, India},
abstract={Heart disease is the prevalent contributor to chronic conditions. The heart disease risk factors affect every region of the world and are the major barriers to sustainable human development. Long-term exposure to heart disease risk factors affects hardest and culminates in death. Although heart disease reached outbreak proportions, yet it is preventable through an early diagnosis of risk factors. To diagnose this lethal disease in advance we use machine learning techniques in medical domain for finding decisive risk factors for heart disease. The heart disease risk assessment model is build to reduce progression to severe and costly illness and complications. The proposed model is developed on Jupyter Notebook web application in anaconda using decision tree, random forest and logistic regression. We conduct various experiments to predict and diagnose the heart disease at its earliest using different metrics performance measures. We compute the recognition rate, precision, recall and AUC for each method and also use out-of-sample testing model validation technique to calculate the impartial measurements. The experimental results reveal that random forest algorithm outperform the logistic regression and decision tree algorithms with an optimal recognition rate, precision, recall and AUC of 0.852, 0.864619, 0.821162 and 0.94. The results carried out in this study are, to the finest of our observation, higher than published values in the literature. Simulation results got from these machine learning techniques prove that the model produces higher accuracy and prove its usefulness in the initial prognosis and examination of heart disease. © 2018, Institute of Advanced Scientific Research, Inc.. All rights reserved.},
author_keywords={Decision tree;  Heart disease;  Logistic regression;  Machine learning techniques;  Random forest},
