@ARTICLE{Olaru201930,
author={Olaru, O.},
title={What is digital humanities and what’s it doing in romanian departments?},
journal={Revista Transilvania},
year={2019},
volume={2019},
number={5-6},
pages={30-37},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85069761562&partnerID=40&md5=bac5e6e18b0b7c45792642337dc4d2f2},
affiliation={Babeș-Bolyai University, Faculty of Letters, Cluj-Napoca, Romania},
abstract={This paper attempts to discuss the emergence of Digital Humanities in the Romanian academic milieu, as well as raise some questions regarding its applicability, both within Romanian research, as well as internationally. The paper then presents a series of possible models for the implementation of digital methods in Romanian philological research, taking into consideration both the technological infrastructure required to sustain this research, as well as a shift in research policies. Furthermore, the paper outlines two possible scenarios, one in which the researcher is not granted access to the digitized corpus he attempts to study and the second, in which he has full access to the metadata by using Python in a browser-hosted Jupyter Notebook. Both these scenarios yield significant results, as the paper shows, but, most important of all, they illustrate the possibility of conducting digital research despite the lack of digital infrastructure, of interinstitutional collaboration and of advanced technical skills. © 2019, Biblioteca Astra Sibiu. All rights reserved.},
