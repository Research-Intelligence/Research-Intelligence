@CONFERENCE{Somerville2020,
author={Somerville, D. and Cutts, Q. and Barr, M. and Parkinson, J.},
title={Addressing mixed levels of prior knowledge by individualising learning pathways in a Degree Apprenticeship Summer School},
journal={ACM International Conference Proceeding Series},
