@CONFERENCE{Urcelay-Olabarria2017458,
author={Urcelay-Olabarria, I. and Lazkoz, R. and Urrestilla, J. and Leonardo, A. and Igartua, J.M.},
title={Jupyter notebook as the physics experimental laboratory's logbook: First approach},
journal={CSEDU 2017 - Proceedings of the 9th International Conference on Computer Supported Education},
year={2017},
volume={1},
pages={458-463},
note={cited By 0; Conference of 9th International Conference on Computer Supported Education, CSEDU 2017 ; Conference Date: 21 April 2017 Through 23 April 2017;  Conference Code:128072},
