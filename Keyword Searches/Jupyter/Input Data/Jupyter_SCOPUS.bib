@ARTICLE{Zhou2017,
author={Zhou, J. and Applegate, C. and Alonso, A.D. and Reynolds, D. and Orford, S. and Mackiewicz, M. and Griffiths, S. and Penfield, S. and Pullen, N.},
title={Leaf-GP: An open and automated software application for measuring growth phenotypes for arabidopsis and wheat},
