@CONFERENCE{Weber2019,
author={Weber, B.W.},
title={Climbing Bloom’s taxonomy with Jupyter notebooks: Experiences in mechanical engineering},
journal={ASME International Mechanical Engineering Congress and Exposition, Proceedings (IMECE)},
year={2019},
volume={5},
doi={10.1115/IMECE2019-10615},
note={cited By 0; Conference of ASME 2019 International Mechanical Engineering Congress and Exposition, IMECE 2019 ; Conference Date: 11 November 2019 Through 14 November 2019;  Conference Code:156960},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85078695890&doi=10.1115%2fIMECE2019-10615&partnerID=40&md5=c41b7782be74ad9d28164b9e8ea27ddd},
affiliation={Department of Mechanical Engineering, University of Connecticut, Storrs, CT  06269, United States},
abstract={Active learning techniques are well known to improve student outcomes and shift their thinking higher on Bloom’s Taxonomy. In this paper, I will present experiences from my implementation of active learning in sophomore-level Mechanical Engineering thermodynamics classes using Jupyter Notebooks. Jupyter Notebooks are a web-based programming environment widely used in software and other industries that allows students to combine code with equations and explanations of their work. By using this software to solve open-ended design problems, students move from the Remember, Comprehend, and Apply steps of Bloom’s Taxonomy to the Analyze, Synthesize, and Evaluate stages. Successfully integrating Jupyter Notebooks with these classes has required a shift in the focus of contact hours, going from didactic presentation of material to active practice with problem-solving techniques; most class material has been moved online. This approach has been successfully implemented over several semesters, but there are nonetheless still improvements to be made, which are discussed at the end of the manuscript. Copyright © 2019 ASME.},
keywords={Artificial intelligence;  Blooms (metal);  Students;  Thermodynamics, Active Learning;  Engineering thermodynamics;  Open-ended designs;  Problem solving technique;  Sophomore-level;  Student outcomes;  Web-based programming, Taxonomies},
