@CONFERENCE{Macdonald201854,
author={Macdonald, C. and McCreadie, R. and Ounis, I.},
title={Agile information retrieval experimentation with Terrier notebooks},
journal={CEUR Workshop Proceedings},
year={2018},
volume={2167},
pages={54-61},
note={cited By 1; Conference of 1st Biennial Conference on Design of Experimental Search and Information Retrieval Systems, DESIRES 2018 ; Conference Date: 28 August 2018 Through 31 August 2018;  Conference Code:138642},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85052613260&partnerID=40&md5=d1fd6d5704fc85efa4a23168cea2b7aa},
affiliation={University of Glasgow, Glasgow, United Kingdom},
abstract={Teaching modern information retrieval is greatly benefited by giving students hands-on experience with an open-source search engine that they can experiment with. As such, open source platforms such as Terrier are a valuable resource upon which learning exercises can be built. However, experimentation using such systems can be a laborious process when performed by hand; queries might be rewritten, executed, and model parameters tuned. Moreover, the rise of learning-to-rank as the de-facto standard for state-ofthe- art retrieval complicates this further, with the introduction of training, validation and testing (likely over multiple folded datasets representing different query types). Currently, students resort to shell scripting to make experimentation easier, however this is far from ideal. On the other hand, the introduction of experimental pipelines in platforms like scikit-learn and Apache Spark in conjunction with notebook environments such as Jupyter have been shown to markedly reduce to barriers to non-experts setting up and running experiments. In this paper, we discuss how next generation information retrieval experimental pipelines can be combined in an agile manner using notebook-style interaction mechanisms. Building upon the Terrier IR platform, we describe how this is achieved using a recently released Terrier-Spark module and other recent changes in Terrier 5.0. Overall, this paper demonstrates the advantages of the agile nature of notebooks to experimental IR environments, from the classroom environment, through academic and industry research labs. © 2018 Copyright held by the author(s).},
keywords={Agile manufacturing systems;  Information retrieval;  Information retrieval systems;  Pipelines;  Teaching, Classroom environment;  De facto standard;  Industry research;  Interaction mechanisms;  Learning to rank;  Model parameters;  Open source platforms;  Open source searches, Search engines},
