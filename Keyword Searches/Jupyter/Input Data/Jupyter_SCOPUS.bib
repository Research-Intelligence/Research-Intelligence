@CONFERENCE{Saygili201819,
author={Saygili, G. and Rathje, E.M. and Wang, Y. and El-Kishky, M.},
title={Cloud-Based Tools for the Probabilistic Assessment of the Seismic Performance of Slopes},
journal={Geotechnical Special Publication},
year={2018},
volume={2018-June},
number={GSP 293},
pages={19-26},
doi={10.1061/9780784481486.003},
note={cited By 0; Conference of 5th Geotechnical Earthquake Engineering and Soil Dynamics Conference: Slope Stability and Landslides, Laboratory Testing, and In Situ Testing, GEESDV 2018 ; Conference Date: 10 June 2018 Through 13 June 2018;  Conference Code:136925},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85048884830&doi=10.1061%2f9780784481486.003&partnerID=40&md5=2b36c28ce8e963eeda4c7ab9ef968740},
affiliation={Dept. of Civil Engineering, Univ. of Texas at Tyler, 3900 University Blvd., Tyler, TX  75799, United States; Dept. of Civil, Architectural, and Environmental Engineering, Univ. of Texas at Austin, 301 E. Dean Keeton St., Austin, TX  78712, United States; College of Civil Engineering and Architecture, Zhejiang Univ., 866 Yuhangtang Rd., Hangzhou, 310058, China; Dept. of Computer Science, Univ. of Texas at Tyler, 3900 Univ. Blvd., Tyler, TX  75799, United States},
abstract={The seismic performance of slopes is typically evaluated based on the sliding displacement predicted to occur along a critical sliding surface. A probabilistic assessment of sliding displacement can account rigorously for the aleatory variability in earthquake ground shaking and in the dynamic response and sliding displacement predictions, providing a more complete assessment of the risk associated with seismic slope failure. Rathje and Saygili (2008) developed probabilistic frameworks for the displacement hazard assessment of rigid sliding masses and Rathje et al. (2014) extended these probabilistic frameworks to deeper and/or softer sliding masses. This paper describes efforts to script the developed frameworks within publicly accessible, cloud-based Jupyter notebooks (www.jupyter.org). The developed Jupyter notebooks use as input the yield acceleration (ky) and natural period (Ts) of the slope, and ground motion hazard information in terms of the hazard curve for peak ground acceleration (PGA) and the associated magnitude and distance deaggregations. The Jupyter notebooks are developed to interface directly with the U.S. Geological Survey Unified Hazard Tool such that the required ground motion hazard information can be input directly from the U.S. Geological Survey website. The developed Jupyter notebooks are available to the public as a published dataset within the Data Depot of DesignSafe (www.designsafe-ci.org), the cyberinfrastructure for the Natural Hazards Engineering Research Infrastructure (NHERI). © 2018 American Society of Civil Engineers.},
funding_details={National Science FoundationNational Science Foundation, CMMI-1520817},
funding_text 1={The work was funded by the DesignSafe project, which is financially supported by the National Science Foundation under grant CMMI-1520817. This support is gratefully acknowledged.},
