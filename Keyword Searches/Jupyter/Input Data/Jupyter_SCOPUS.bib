@CONFERENCE{Gurram2017,
author={Gurram, H. and Subramanian, C. and Pinelli, J.-P. and Basu, R.},
title={Processing data from hurricane matthew in Designsafe-Ci},
journal={2017 Americas Conference on Wind Engineering, ACWE 2017},
year={2017},
note={cited By 0; Conference of 2017 13th Americas Conference on Wind Engineering, ACWE 2017 ; Conference Date: 21 May 2017 Through 24 May 2017;  Conference Code:130163},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85029814158&partnerID=40&md5=0d1ba7e7c4cb1390ff47a75e8c6e7d6c},
affiliation={Mechanical and Aerospace Engineering Department, Florida Tech., Melbourne, FL, United States; Civil Engineering Department, Florida Tech., Melbourne, FL, United States},
abstract={The DesignSafe.ci is the cyber-infrastructure component of the Natural Hazard Engineering Research Infrastructure (NHERI) funded by the National Science Foundation. It provides a cloud platform to all researchers to archive and share their data on the public domain. It supports research workflows, data analysis and visualization, and the full lifecycle of data required by engineers and scientists, to effectively deal with the threats posed to civil infrastructure by natural hazards. The researchers from Florida Institute of Technology deployed their wireless sensor network (WSN) system on the roof top of a house near the Satellite beach, Melbourne, Florida (longitude and latitude: 28.175944, -80.592710) during the October 6-8, 2017 hurricane Matthew passage to collect real time data. The WSN is an autonomous full scale hurricane data measurement system with 30 pressure and temperature sensors, and an anemometer to measure pressure, temperature, speed and wind direction respectively. All sensors are connected wirelessly through 3 routers to a base modem, which is connected to the laptop for collecting the data. The data from the laptop was uploaded through a cellular network at five minute interval to a Box account, which provides cloud storage and file sharing collaboration. The entire wireless sensor network system was monitored using Team Viewer remote desktop application. All the data collected in the Box was then uploaded into the DesignSafe-ci workspace, which can be shared with researchers. For future deployments, these processes can be performed using the DesignSafe-ci workspace. For post processing and analysis of the uploaded data, DesignSafe-ci provides software tools like Jupyter and MATLAB. To perform the analysis a unique graphical user interface (GUI) application was created using MATLAB, which is capable of analyzing the complete data set on a single run. The authors can run the GUI code instantly on the cloud and provide plots of the physical data, thanks to the virtual desktop connection to the DesignSafe workspace. This paper describes how De-signSafe-ci facilitates the setup, remote monitoring and analysis of the data processing collected during a hurricane. The post processing capabilities for analyzing Hurricane Mathew data are illustrated. A companion paper describes a more detailed analysis of the data.},
author_keywords={DesignSafe-ci;  GUI;  Hurricane matthew;  JUPYTER;  MATLAB;  NHERI;  Wireless sensor network},
keywords={Data acquisition;  Data visualization;  Digital storage;  Distributed computer systems;  Graphical user interfaces;  Hazards;  Hurricanes;  Laptop computers;  MATLAB;  User interfaces;  Wireless sensor networks, Cyber infrastructures;  DesignSafe-ci;  Graphical user interfaces (GUI);  JUPYTER;  National Science Foundations;  NHERI;  Pressure and temperature sensors;  Research infrastructure, Data handling},
